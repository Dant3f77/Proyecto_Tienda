@{
    ViewBag.Title = "Alta";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous">
    <title>Alta</title>
</head>
<body>
    <div>

        <form method="post" action="/DetalleFactura/Alta">
            <label>
                Id:
                <input type="text" class="mb-2" name="Id" />
            </label>
            <br />
            <label>
                IdDetalle:
                <input type="text" class="mb-2" name="IdDetalle" />
            </label>
            <br />
            <label>
                Precio:
                <input type="text" class="mb-2" name="Precio" />
            </label>
            <br />
            <label>
                Cantidad:
                <input type="text" class="mb-2" name="Cantidad" />
            </label>
            <br />
            <label>
                IdProducto:
                <input type="text" class="mb-2" name="IdProducto" />
            </label>
            <br />
            <label>
                IdFactura:
                <input type="text" class="mb-2" name="IdFactura" />
            </label>
            <br />
           

            <input type="submit" value="Confirmar" />
        </form>
    </div>
</body>
</html>